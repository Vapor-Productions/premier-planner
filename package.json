{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "setup:prod": "yarn --omit=dev && yarn prisma:generate && yarn prisma:push",
    "setup": "yarn && yarn prisma:generate && yarn prisma:push",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "build": "tsc",
    "dev": "tsx src/dev.ts",
    "watch": "nodemon --ext ts,js,mjs,cjs,json --exec yarn dev",
    "biome:fix": "biome check --write . && biome format --write .",
    "start": "node build/main.js",
    "start:prod": "tsc && node build/main.js",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format .",
    "format:fix": "biome format --write ."
  },
  "dependencies": {
    "@discordx/importer": "^1.3.1",
    "@discordx/utilities": "^7.0.5",
    "@prisma/client": "^6.17.0",
    "axios": "^1.12.2",
    "discord.js": "^14.21.0",
    "discordx": "^11.12.2",
    "dotenv": "^17.0.1",
    "pg": "^8.16.3",
    "prisma": "^6.17.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@types/node": "^22.10.10",
    "chokidar": "^4.0.3",
    "tsx": "^4.20.3",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=7.0.0"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  }
}
